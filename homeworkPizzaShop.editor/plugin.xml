-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated homeworkPizzaShop -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="homeworkPizzaShop.presentation.HomeworkPizzaShopModelWizardID"
            name="%_UI_HomeworkPizzaShopModelWizard_label"
            class="homeworkPizzaShop.presentation.HomeworkPizzaShopModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/HomeworkPizzaShopModelFile.gif">
         <description>%_UI_HomeworkPizzaShopModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated homeworkPizzaShop -->
      <editor
            id="homeworkPizzaShop.presentation.HomeworkPizzaShopEditorID"
            name="%_UI_HomeworkPizzaShopEditor_label"
            icon="icons/full/obj16/HomeworkPizzaShopModelFile.gif"
            extensions="homeworkpizzashop"
            class="homeworkPizzaShop.presentation.HomeworkPizzaShopEditor"
            contributorClass="homeworkPizzaShop.presentation.HomeworkPizzaShopActionBarContributor">
      </editor>
   </extension>

</plugin>
